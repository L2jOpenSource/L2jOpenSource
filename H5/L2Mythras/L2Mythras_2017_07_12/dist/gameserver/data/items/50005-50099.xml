<armor id="50005" name="Knights Cloak">
    <set name="enchantable" value="false"/>
    <set name="crystal_type" value="S84"/>
    <set name="price" value="0"/>
    <set name="type" value="NONE"/>
    <set name="weight" value="220"/>
    <equip>
      <slot id="BACK"/>
    </equip>
    <attributes>
      <attribute element="fire" value="20"/>
      <attribute element="water" value="20"/>
      <attribute element="wind" value="20"/>
      <attribute element="earth" value="20"/>
      <attribute element="holy" value="20"/>
      <attribute element="unholy" value="20"/>
	</attributes>
    <for>
      <add stat="pDef" order="0x10" value="30"/>
      <enchant stat="pDef" order="0x0C" value="0"/>
      <enchant stat="maxHp" order="0x80" value="0"/>
    </for>
  </armor>

